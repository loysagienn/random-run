  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clear-dist": "rm -rf './dist'",
    "build-server-dev": "npm run clear-dist && NODE_ENV=development BABEL_ENV=development babel src --extensions \".js,.jsx,.ts,.tsx\" --out-dir dist -w",
    "build-server": "npm run clear-dist && NODE_ENV=production BABEL_ENV=production babel src --extensions \".js,.jsx,.ts,.tsx\" --out-dir dist",
    "build-client-dev": "NODE_ENV=development npx webpack --watch --progress",
    "build-client": "NODE_ENV=production npx webpack",
    "build-dev": "npm run build-server-dev & npm run build-client-dev",
    "build": "npm run build-server & npm run build-client",
    "start-dev": "NODE_ENV=development nodemon dist/server/index.js",
    "start": "NODE_ENV=production node dist/server/index.js"
  },
